<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.library.controller.StartingPageController" spacing="10" alignment="CENTER">
    <Button text="Login" fx:id="loginButton" onAction="#handleLogin"/>
    <Button text="Sign Up" fx:id="signUpButton" onAction="#handleSignUp"/>
    <Button text="See the 5 Best Rated Books" fx:id="seeBestBooksButton" onAction="#handleSeeBestBooks"/>
</VBox>
